<seelog minlevel="trace">
    <outputs>
        <splitter formatid="default">
            <console/>
            <!-- <rollingfile type="date" filename="./log/dlagent.log" datepattern="20060102" fullname="true" maxrolls="31" /> -->
            <buffered size="10000" flushperiod="1000">
                <rollingfile type="date" filename="./log/dlagent.log" datepattern="20060102" fullname="true" maxrolls="31" />
            </buffered>
        </splitter>
